set smarttab
set shiftwidth=2
set tabstop=2
set number
syntax enable
let g:solarized_termtrans = 1
let g:solarized_termcolors = 16 
let g:solarized_contrast = "high"
set background=dark
colorscheme solarized 
highlight Normal ctermfg=White
highlight Comment ctermfg=White
highlight Constant ctermfg=White
